<% provide(:title, @user.name) %>
<h1>
<%= gravatar_for @user %>
<%= @user.name %>
</h1>
<% if signed_in? %>
  <section>
    <%= render 'shared/subject_form' %>
  </section>
  <input id="save" type="button" value="Save Events">
  <textarea id="debug_text">test</textarea>
  <% if have_subject?(@user) %>
    <%= render 'shared/subject' %>
    <div id=calendar>Calendar</div>
  <% end %>
<% end %>


