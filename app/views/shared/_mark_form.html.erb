<%= form_for(@mark) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_field :date %>
    <%= f.text_field :subject_id %>
  </div>
  <%= get_subject_id @user, "english" %>
  <%= f.submit "Save Events", class: "btn btn-large btn-primary" %>
<% end %>
